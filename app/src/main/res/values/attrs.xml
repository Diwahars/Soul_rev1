<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="MetaballMenu">
        <attr name="backgroundColor" format="color" />
        <attr name="metaballColor" format="color" />
        <attr name="backgroundShapeRadius" format="dimension" />
        <attr name="needsElevation" format="boolean" />
    </declare-styleable>


    <declare-styleable name="MetaballMenuImageView">
        <attr name="defaultImage" format="reference"/>
        <attr name="selectedImage" format="reference"/>
    </declare-styleable>


    <declare-styleable name="SpaceNavigationView">
        <attr name="space_item_icon_size" format="dimension" />
        <attr name="space_item_icon_only_size" format="dimension" />
        <attr name="space_item_text_size" format="dimension" />
        <attr name="space_background_color" format="color" />
        <attr name="centre_button_color" format="color" />
        <attr name="active_item_color" format="color" />
        <attr name="inactive_item_color" format="color" />
        <attr name="centre_button_icon" format="reference" />
        <attr name="centre_button_icon_color" format="color" />
    </declare-styleable>

        <declare-styleable name="ArcMenu">
            <attr name="hintSize" format="dimension|reference" />
            <attr name="childSize" format="dimension|reference" />
            <attr name="hintNormalColor" format="string" />
            <attr name="hintPressColor" format="string" />
            <attr name="hintUpperMarkColor" format="string" />
            <attr name="hintText" format="string" />
            <attr name="hintTextSize" format="dimension" />
            <attr name="rotateInClosing" format="boolean" />
            <attr name="shadowBorderWidth" format="dimension" />
            <attr name="shadowBorderHeight" format="dimension" />
            <attr name="shadowElevation" format="dimension" />
            <attr name="shadowColor" format="string" />
            <attr name="shadowAroundColor" format="string" />
            <attr name="shadowMargin" format="dimension" />
            <attr name="hintTopImage" format="integer"/>
            <attr name="hintGravity" format="integer">
                <enum name="Top_Left" value="1" />
                <enum name="Top_Right" value="2" />
                <enum name="Top_Middle" value="3" />
                <enum name="Bottom_Left" value="4" />
                <enum name="Bottom_Right" value="5" />
                <enum name="Bottom_Middle" value="6" />
                <enum name="Right_Middle" value="7" />
                <enum name="Left_Middle" value="8" />
                <enum name="Center" value="9" />
            </attr>
        </declare-styleable>

        <declare-styleable name="RayLayout">
            <attr name="holderWidth" format="dimension" />
            <attr name="MenuDirection" format="integer">
                <enum name="Up_to_Down" value="1" />
                <enum name="Left_to_Right" value="2" />
                <enum name="Down_to_Up" value="3" />
                <enum name="Right_to_Left" value="4" />
            </attr>
        </declare-styleable>

    <declare-styleable name="GooeyMenu">

        <attr name="no_of_menu" format="integer" />
        <attr name="fab_radius" format="dimension" />
        <attr name="menu_radius" format="dimension" />
        <attr name="gap_between_menu_fab" format="dimension" />
        <attr name="menu_drawable" format="reference" />
    </declare-styleable>
    </resources>
